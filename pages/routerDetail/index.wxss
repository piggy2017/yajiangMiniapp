}
.router_box_left_img{
  width: 140rpx;
  height: 140rpx;
  border-radius: 100%;
  background-color: #fff;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.router_box_left_img image{
  width: 130rpx;
  height: 130rpx;
   border-radius: 100%;
   border: 3px solid #fff;
}
.router_box_left_line{
  width: 100%;
  height: 100rpx;
  display: flex;
align-items: center;
justify-content: center;
}
.router_box_left_line>view{
  width: 6rpx;
  height: 100rpx;
  background-color: #fff;
}
.router_box_right{
  width: 70%;
  height:calc(240rpx + 3px);
  background-color: #fff;
  border-radius: 15rpx;
}
.router_box_right_content{
  height: calc(240rpx + 3px);
  position: relative;
  display: flex;
  flex-wrap: wrap;
  align-content: center;
}
.router_box_right_content .content{
  width: 90%;
  margin: 0rpx auto;
}
.router_box_right_content .content:nth-of-type(2){
  overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.router_box_right_content .content:nth-of-type(3){
  font-size: 26rpx;
  color: #222;
  margin: 20rpx auto;
}
.router_box_right_content .content:nth-of-type(4){
  font-size: 26rpx;
  color: #666666;
  text-overflow: -o-ellipsis-lastline;
overflow: hidden;
text-overflow: ellipsis;
display: -webkit-box;
-webkit-line-clamp: 2;
line-clamp: 2;
-webkit-box-orient: vertical;
}
