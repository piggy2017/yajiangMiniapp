/* pages/routerDetail/index.wxss */

.container {
  background-color: #ecedf2;
}

.custom {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 45px;
  background-color: #fff;
  z-index: 999;
}

.custom-content {
  width: calc(74% - 60rpx);
  height: 100%;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  justify-content: space-between;
}

.custom-back-img {
  width: 20rpx;
  padding-right: 20rpx;
  height: 34rpx;
}

.custom-like-img {
  width: 34rpx;
  height: 28rpx;
  padding-right: 20rpx;
}

.custom-share-img {
  width: 30rpx;
  height: 34rpx;
}

.custom .custom-name {
  color: #222;
  font-size: 17px;
  font-weight: 500;
  text-align: center;
  width: calc(100% - 120rpx);
}

.nav {
  height: 84rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  background: #fff;
  font-size: 16px;
  white-space: nowrap;
  position: fixed;
  top: 70px;
  left: 0;
  z-index: 100;
  padding-left: 30rpx;
}

.nav-item {
  width: 20%;
  display: inline-block;
  text-align: center;
}

.nav-item.active {
  color: #5D7AFF;
  border-bottom: 2px solid #5D7AFF;
}

