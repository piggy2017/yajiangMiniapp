<swiper class="swiper" indicator-active-color="#fff" indicator-color="#000" indicator-dots="true" circular="true" autoplay="true" interval="5000" duration="1000" style="height:{{sHeight}};">    
  <block wx:for="{{shufflingList}}" wx:for-item="item" wx:key="key">    
    <swiper-item>     
      <image src="{{item.imageUrl}}" class="slide-image" mode="aspectFill" bindtap="imagePreview"/>    
    </swiper-item>    
  </block>    
</swiper> 